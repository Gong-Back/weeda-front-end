{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "typeRoots": [],
    //"rootDir": "src",
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/apis/*": ["./src/apis/*"],
      "@/components/*": ["./src/components/*"],
      "@/screens/*": ["./src/screens/*"],
      "@/constants/*": ["./src/constants/*"],
      "@/routes/*": ["./src/routes/*"],
      "@/assets/*": ["./assets/*"]
    }
  },
  "include": ["src", "declarations.d.ts"]
}
